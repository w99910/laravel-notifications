{
    "name": "thomasbrillion/notification",
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/events": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/mail": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/validation": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Thomasbrillion\\Notification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thomasbrillion\\Notification\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thomasbrillion\\Notification\\NotificationServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "authors": [
        {
            "name": "Thomas",
            "email": "hello@thomasbrillion.pro"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}