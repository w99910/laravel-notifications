{
    "name": "thomasbrillion/notification",
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4|^8.5",
        "illuminate/http": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/events": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/mail": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/validation": "^8.12|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.0",
        "laravel/framework": "^8.12"
    },
    "autoload": {
        "psr-4": {
            "Thomasbrillion\\Notification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thomasbrillion\\Notification\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thomasbrillion\\Notification\\NotificationServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "authors": [
        {
            "name": "Thomas",
            "email": "hello@thomasbrillion.pro"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}